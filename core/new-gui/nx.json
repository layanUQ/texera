{
  "npmScope": "texera-gui",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "e2e"
        ],
        "runtimeCacheInputs": [
          "node -v",
          "cat ./src/environments/environment.prod.ts | shasum"
        ],
        "accessToken": "MWY5NjA0ZDAtNTdjOS00NDY3LWIzNjYtNTYyMWU5OGFkYjgzfHJlYWQtd3JpdGU=",
        "parallel": 1
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@angular-eslint/schematics"
  },
  "defaultProject": "texera-gui",
  "generators": {
    "@schematics/angular:component": {
      "prefix": "texera",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "texera"
    }
  }
}
